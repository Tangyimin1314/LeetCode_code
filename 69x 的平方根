class Solution {
public:
    int mySqrt(int x) {
       int low=0,high=x,result=-1;
       while(low<=high){
           int mid=low+(high-low)/2;
           if((long long)mid*mid<=x){
               result=mid;
               low=mid+1;
           }else{
               high=mid-1;
           }
       }
       return result;
    }
};
