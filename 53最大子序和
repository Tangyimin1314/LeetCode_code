class Solution {
public:
    int maxSubArray(vector<int>& nums) {
        int max=nums[0];
        int temp=0;
        for(auto num:nums){
            temp+=num;
            if(temp<0)
                temp=0;
            else if(temp>max)
                max=temp;
            if(num>max)max=num;
        }
        return max;
    }
};
